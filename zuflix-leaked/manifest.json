{
  "manifest_version": 3,
  "name": "#1 Rayzu Extension for Netflix",
  "version": "1.0",
  "permissions": [
    "cookies",
    "tabs",
    "activeTab",
    "storage",
    "cookies",
    "webNavigation",
    "https://www.netflix.com/*"
  ],
  "host_permissions": [
    "https://www.netflix.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
